#include <stdio.h>
#include <string.h>

int main() {
    
    char name[999];
    int i, length;
    
    printf("Enter username: ");
    gets(name);
    
    length = strlen(name);
    
    printf("Your jejemon username is: ");
    for(i=0;i<length;i++){
        if(i%2==0){
        printf("%c",tolower(name[i]));
        }
        else{
        printf("%c",toupper(name[i]));
        }
    }

    return 0;
}
