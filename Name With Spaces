#include <stdio.h>

int main() {
    char str[1000];
    int len,i;
    
    printf("Enter Name: ");
    scanf("%[^\n]s",str);
    
    for(len=0;str[len]!='\0';len++);
    
    for(i=0;i<len;i++){
        printf("%c",str[i]);
        if(i==len-1){
            break;
        }
        printf(" ");
        }

    return 0;
}
